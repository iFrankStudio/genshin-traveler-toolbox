{
  "name": "genshin-traveler-toolbox",
  "version": "0.0.1",
  "description": "A toolbox for Genshin Impack. Build with Electron and Vue3.",
  "repository": "github:iFrankStudio/genshin-traveler-toolbox",
  "homepage": "https://github.com/iFrankStudio/genshin-traveler-toolbox",
  "keywords": [
    "genshin",
    "genshin impact",
    "mihoyo",
    "gacha",
    "traveler",
    "paimon",
    "toolbox"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "build:dir": "cross-env BUILD_TARGET=dir node scripts/build.js",
    "build:lite": "cross-env BUILD_TARGET=lite node scripts/build.js",
    "build:production": "cross-env BUILD_TARGET=production node scripts/build.js",
    "build:debug": "cross-env BUILD_TARGET=debug node scripts/build.js",
    "test": "playwright test",
    "lint": "eslint --ext .ts,.vue,.js src scripts",
    "lint:fix": "eslint --ext .ts,.vue,.js src scripts --fix",
    "postinstall": "electron-rebuild && node ./scripts/dev.install.js",
    "reinstall": "electron-rebuild",
    "run:dir": "electron ./dist"
  },
  "author": {
    "email": "ifrankstudio@gmail.com",
    "name": "iFrank Studio"
  },
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/typography": "^0.4.1",
    "core-js": "^3.18.1",
    "echarts": "^5.2.1",
    "electron-updater": "^4.3.9",
    "vue": "^3.2.19",
    "vue-echarts": "^6.0.0",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "external": [],
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/url-parse": "^1.4.4",
    "@vicons/fa": "^0.11.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "autoprefixer": "^10.3.6",
    "builtin-modules": "^3.2.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "electron": "^13.5.0",
    "electron-builder": "22.13.1",
    "electron-log": "^4.4.1",
    "electron-rebuild": "^3.2.3",
    "electron-unhandled": "^3.0.2",
    "electron-window-state": "^5.0.3",
    "esbuild": "^0.13.2",
    "extract-zip": "^1.7.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "ini": "^2.0.0",
    "lodash-es": "^4.17.21",
    "magic-string": "^0.25.7",
    "moment": "^2.29.1",
    "naive-ui": "^2.19.3",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "regedit": "^5.0.0",
    "rollup": "^2.57.0",
    "sudo-prompt": "^9.2.1",
    "tailwindcss": "^2.2.16",
    "tslib": "^2.3.1",
    "type-fest": "^2.3.4",
    "typescript": "^4.4.3",
    "url-parse": "^1.5.3",
    "vfonts": "^0.1.0",
    "vite": "^2.5.10",
    "vite-plugin-style-import": "^1.2.1",
    "vite-svg-loader": "^2.2.0"
  },
  "optionalDependencies": {
    "@playwright/test": "^1.15.0",
    "@types/ini": "^1.3.30",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.10.1",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.18.0",
    "playwright": "^1.15.0",
    "prettier": "^2.4.1"
  },
  "build": {
    "appId": "com.ifrankstudio.genshin-traveler-toolbox",
    "extraResources": [
      {
        "from": "node_modules/regedit/vbs",
        "to": "regedit/vbs",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
